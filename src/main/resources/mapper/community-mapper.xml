<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "htttp://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.iqiu_backend.dao.CommunityMapper">

    <select id="selectPostsCountByCommunityId" resultType="int">
        select count(*)
        from posts
        where community_id = #{communityId}
    </select>
</mapper>